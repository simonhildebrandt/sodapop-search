{
  "name": "sodapop-search",
  "version": "1.0.0",
  "main": "lib/eleventy-index.mjs",
  "repository": "git@github.com:simonhildebrandt/sodapop-search.git",
  "author": "Simon Hildebrandt <simonhildebrandt@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.4",
    "lunr": "^2.3.9",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "esbuild": "^0.27.3",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1"
  },
  "scripts": {
    "build:css": "postcss src/sodapop-search.css -o lib/sodapop-search.css",
    "watch:css": "postcss src/sodapop-search.css -o lib/sodapop-search.css -w",
    "build:react": "yarn esbuild src/index.jsx --bundle --outfile=lib/sodapop-search.js",
    "watch:react": "yarn esbuild src/index.jsx --bundle --outfile=lib/sodapop-search.js --watch",
    "build:search": "yarn esbuild src/eleventy-index.mjs --bundle --outfile=lib/eleventy-index.js",
    "watch:search": "yarn esbuild src/eleventy-index.mjs --bundle --outfile=lib/eleventy-index.js --watch"
  }
}
